    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "start": "node ./node_modules/.bin/webpack-dev-server --hot --inline",
    "clean": "rimraf build",
    "build:webpack": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --config webpack.prod.js --progress --colors --bail",
